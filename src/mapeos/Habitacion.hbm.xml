<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.unla.datos.Habitacion" table="Habitacion">
		<id column="idHabitacion" name="idHabitacion">
			<generator class="identity" />
		</id>
		<property column="capacidadMaxima" name="capacidadMaxima" type="int" />
		<property column="disponible" name="disponible" type="boolean" />
		<property column="precio" name="precio" type="double" />
	</class>
</hibernate-mapping>

