<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.unla.datos.ReporteMantenimiento" table="reportemantenimiento">
		<id column="idReporteMantenimiento" name="idReporteMantenimiento">
			<generator class="identity" />
		</id>
		<property column="fechaOrigen" name="fechaOrigen" type="calendar" />
		<property column="fechaResolucion" name="fechaResolucion" type="calendar" />
		<property column="reparacion" name="reparacion" type="string" />
		<property column="desperfecto" name="desperfecto" type="string" />
		<property column="terminado" name="terminado" type="boolean" />
		<!-- Relaciones -->
		<many-to-one name="empleadoACargo" column="legajo" class="com.unla.datos.Empleado"></many-to-one>
	</class>
</hibernate-mapping>

