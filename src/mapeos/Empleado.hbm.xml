<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.unla.datos.Empleado" table="empleado">
		<id column="legajo" name="legajo">
			<generator class="identity" />
		</id>
		<property column="nombre" name="nombre" type="string" />
		<property column="apellido" name="apellido" type="string" />
		<property column="dni" name="dni" type="int" />
		<property column="fechaNacimiento" name="fechaNacimiento"
			type="calendar" />
		<many-to-one name="login" column="idLogin" class="com.unla.datos.Login" unique="true" not-null="true"/>
		<many-to-one name= "contacto" column="idContacto"  class="com.unla.datos.Contacto" unique="true" not-null="true" />
		<many-to-one name= "tipo" column="idTipoEmpleado"  class="com.unla.datos.TipoEmpleado" unique="true" not-null="true" />


	</class>
</hibernate-mapping>