<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.unla.datos.TipoTicket" table="tipoticket">
		<id column="idTipoTicket" name="idTipoTicket">
			<generator class="identity" />
		</id>
		<property column="descripcion" name="descripcion" type="string" />
		<property column="precio" name="precio" type="long" />
		
		<set name="tickets" table="ticket" order-by="idTicket asc" inverse="true" lazy="true" fetch="select">
			<key column="idTipoTicket" not-null="true" />
			<one-to-many class="com.unla.datos.Ticket" />
		</set>
		
	</class>
</hibernate-mapping>