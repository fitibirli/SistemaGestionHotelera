<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.unla.datos.Ticket" table="ticket">
		<id column="idTicket" name="idTicket">
			<generator class="identity" />
		</id>
		
		<property column="descripcion" name="descripcion" type="string" />
		<property column="precio" name="precio" type="int" />
		
		<many-to-one name="estadia" class="com.unla.datos.Estadia"></many-to-one>
<!--  
		<one-to-one name="factura" class="com.unla.datos.Factura"></one-to-one>
		<one-to-one name="legajo" class="com.unla.datos.Empleado"></one-to-one>
		<one-to-one name="tipoticket" class="com.unla.datos.TipoTicket"></one-to-one>
-->
	</class>
</hibernate-mapping>