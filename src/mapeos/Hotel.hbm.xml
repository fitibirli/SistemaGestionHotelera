<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.unla.datos.Hotel" table="hotel">
		<id column="idHotel" name="idHotel">
			<generator class="identity" />
		</id>
		<property column="cuil" name="cuil" type="string" />
		<property column="nombre" name="nombre" type="string" />
		<property column="categoria" name="categoria" type="int" />
		<property column="checkIn" name="checkIn" type="calendar" />
		<property column="checkOut" name="checkOut" type="calendar" />
		<many-to-one name= "contacto" column="idContacto"  class="com.unla.datos.Contacto" unique="true" not-null="true" />
		<many-to-one name= "direccion" column="idDireccion"  class="com.unla.datos.Direccion" unique="true" not-null="true"  />

	</class>
</hibernate-mapping>

