<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.unla.datos.ReporteLimpieza" table="reportelimpieza">
		<id column="idReporteLimpieza" name="idReporteLimpieza">
			<generator class="identity" />
		</id>
		<property column="fecha" name="fecha" type="calendar" />
		<property column="desperfecto" name="desperfecto" type="string" />
		<!-- Relaciones -->
		<many-to-one name="legajo" column="legajo" class="com.unla.datos.Empleado"></many-to-one>
	</class>
</hibernate-mapping>