<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.unla.datos.Privilegio" table="privilegio">
		<id column="idPrivilegio" name="idPrivilegio">
			<generator class="identity" />
		</id>
		<property column="descripcion" name="descripcion" type="string" />
		
		<many-to-one name="estado" column="idLogin" outer-join="false" lazy="false" not-found="ignore"
        class="com.unla.datos.Login" >
    </many-to-one>
	</class>
</hibernate-mapping>