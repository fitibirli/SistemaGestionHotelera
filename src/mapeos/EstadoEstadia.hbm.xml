<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.unla.datos.EstadoEstadia" table="estadoestadia">
		<id column="idEstadoEstadia" name="idEstadoEstadia">
			<generator class="foreign" >
				<param name="property">estadia</param>
			</generator>
		</id>
		<one-to-one name="estadia" class="com.unla.datos.Estadia" ></one-to-one>
	</class>
</hibernate-mapping>