<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.unla.datos.EstadoEstadia" table="estadoestadia"
		polymorphism="implicit">
		<!--<cache usage="read-only" /> -->
		<id name="idEstadoEstadia" column="idEstadoEstadia">
			<generator class="identity" />
		</id>
		<discriminator insert="false" column="idEstadoEstadia" />
		<!-- <property name="nombre" column="nombre" /> -->
		<subclass name="com.unla.datos.EstadoReservada"
			discriminator-value="0" />
		<subclass name="com.unla.datos.EstadoEnCurso"
			discriminator-value="1" />
		<subclass name="com.unla.datos.EstadoConfirmada"
			discriminator-value="2" />
		<subclass name="com.unla.datos.EstadoTerminada"
			discriminator-value="3" />
	</class>
</hibernate-mapping>