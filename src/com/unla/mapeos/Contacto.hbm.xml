<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="com.unla.datos.Contacto" table="contacto" >
	<id name="idContacto" column="idContacto">
	<generator class="foreign">
	<param name="property">cliente</param>
	<param name="property">hotel</param>
	<param name="property">empleado</param>
	</generator>
	</id>
	<property name="telefono" type="string" column="telefono" />
	<property name="celular" type="string" column="celular" />
	<property name="email" type="string" column="email" />
	<one-to-one name="cliente" class="com.unla.datos.Cliente" ></one-to-one>
	<one-to-one name="hotel" class="com.unla.datos.Hotel" ></one-to-one>
	<one-to-one name="empleado" class="com.unla.datos.Empleado" ></one-to-one>
	</class>
</hibernate-mapping>