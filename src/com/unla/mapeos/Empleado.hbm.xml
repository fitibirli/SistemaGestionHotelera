<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="datos.Empleado" table="Empleado">
		<id column="legajo" name="legajo">
			<generator class="identity" />
		</id>
		<property column="nombre" name="nombre" type="string" />
		<property column="apellido" name="apellido" type="string" />
		<property column="dni" name="dni" type="long" />
		<property column="fechaNacimiento" name="fechaNacimiento" type="calendar" />
		<property column="baja" name="baja" type="boolean" />
	</class>
</hibernate-mapping>