<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="datos.ReporteLimpieza" table="reporteLimpieza">
		<id column="idReporteLimpieza" name="idReporteLimpieza">
			<generator class="identity" />
		</id>
		<property column="fecha" name="fecha" type="calendar" />
		<property column="desperfecto" name="desperfecto" type="string" />
	</class>
</hibernate-mapping>